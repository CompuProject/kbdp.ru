.item.wline:after{background: @bcolor;}

.dark-nav2 .flex-control-paging li a:hover, .dark-nav2 .flex-control-paging li a.flex-active{background: @bcolor;}
.banners-big .nav-carousel .flex-direction-nav li:hover{background: @bcolor;}

/*review*/
.reviews.item-views.front .item .image:before{background-color: @bcolor;}

/*staff*/
.item-views .item .bottom-props .value{background-color: @bcolor;}
.item-views .item .bottom-props > a:hover{background-color: @hcolor;}

/*filter*/
.filter-action.active, .display-type .view-button:hover, .display-type .view-button.cur{background-color: @bcolor;}
.bx_filter .bx_ui_slider_range, .bx_filter .bx_ui_slider_pricebar_V, .bx_filter.bx_filter_vertical .bx_filter_popup_result{background-color:@bcolor;}
.bx_filter .bx_ui_slider_track .bx_ui_slider_handle{border-color:@bcolor;}
.catalog.bx_filter .bx_filter_parameters_box_title:hover:before, .catalog.bx_filter .bx_filter_parameters_box_title:hover{color:@bcolor;}
.bx_filter input[type="checkbox"] + label:before, .filter input[type="radio"] + label:before, .bx_filter .bx_filter_select_arrow, .common_select .common_select-link .trigger{background-color:@bcolor;}
.bx_filter input[type="checkbox"]:checked + label:after, .filter input[type="checkbox"]:checked + label:after{background-color:@bcolor;border-color:@bcolor;}
.bx_filter_section .btn.btn-transparent{color:@bcolor;}


/*subscribe*/
.subscribe-edit-main .form input[type=checkbox] + label:hover:after, .subscribe-edit-main .form input[type=checkbox]:checked + label:after, .subscribe-edit-main .form input[type=radio] + label:hover:after, .subscribe-edit-main .form input[type=radio]:checked + label:after{ background-color: @bcolor; border-color:@bcolor; }
.subscribe-edit-main font.notetext{border-color:@bcolor;}

@media (max-width:767px){
	.basket_top .icon .count{background:@bcolor!important;}
}


/* h1 & breadcrumbs */
